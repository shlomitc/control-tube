{
    "name": "control-tube",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Control your youtube tab from everywhere in your browser",
    "homepage_url": "http://extensionizr.com",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "page": "src/bg/background.html",
        "persistent": true,
        "tabs": true
    },
    "permissions": [
        "*://*.youtube.com/watch*"
    ]
}